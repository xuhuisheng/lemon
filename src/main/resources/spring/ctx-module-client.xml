<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="rpcAuthHelper" class="${rpc.auth.helper}" />

    <!-- user -->
    <bean id="userClient" class="${client.user}" />

    <bean id="employeeClient" class="${client.employee}" />

    <!-- authn -->
    <bean id="authnClient" class="${client.authn}" />
    
    <!-- authz -->
    <bean id="authzClient" class="${client.authz}" />

    <bean id="authzResourceClient" class="${client.authzresource}" />

    <!-- store -->
    <bean id="storeClient" class="${client.store}" />

    <!-- oss -->
    <bean id="ossClient" class="${client.oss}" />

    <!-- sys -->
    <bean id="sysClient" class="${client.sys}" />

    <!-- open -->
    <bean id="openClient" class="${client.open}" />

    <!-- audit -->
    <bean id="auditClient" class="${client.audit}" />

    <!-- notification -->
    <bean id="notificationClient" class="${client.notification}"/>

    <!-- sendmsg -->
    <bean id="sendMsgClient" class="${client.sendmsg}" />

    <!-- sendmail -->
    <bean id="sendmailClient" class="${client.sendmail}" />

    <!-- calendar -->
    <bean id="calendarClient" class="${client.calendar}" />

    <!-- tenant -->
    <bean id="tenantHolder" class="${tenant.holder}" />

    <bean id="tenantConnector" class="com.mossle.api.tenant.MockTenantConnector" />

    <!-- menu -->
    <bean id="menuConnector" class="${menu.connector}" />

</beans>
