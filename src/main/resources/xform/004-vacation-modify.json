{
	"code":"vacation-modify",
	"name":"调整申请",
	"sections":[{
		"type":"text",
		"tag":"h1",
		"text":"调整申请"
	},{
		"type":"grid",
		"row":"7",
		"col":"4",
		"merge":[{
			"startId":"0-1",
			"minRow":0,
			"minCol":1,
			"maxRow":0,
			"maxCol":3
		},{
			"startId":"1-1",
			"minRow":1,
			"minCol":1,
			"maxRow":1,
			"maxCol":3
		},{
			"startId":"2-2",
			"minRow":2,
			"minCol":2,
			"maxRow":2,
			"maxCol":3
		},{
			"startId":"3-2",
			"minRow":3,
			"minCol":2,
			"maxRow":3,
			"maxCol":3
		},{
			"startId":"4-1",
			"minRow":4,
			"minCol":1,
			"maxRow":4,
			"maxCol":3
		},{
			"startId":"5-1",
			"minRow":5,
			"minCol":1,
			"maxRow":5,
			"maxCol":3
		},{
			"startId":"6-1",
			"minRow":6,
			"minCol":1,
			"maxRow":6,
			"maxCol":3
		}],
		"fields":[{
			"type":"label",
			"row":0,
			"col":0,
			"text":"请假原因"
		},{
			"type":"textfield",
			"row":0,
			"col":1,
			"name":"reason",
			"required":true,
			"readOnly":false
		},{
			"type":"label",
			"row":1,
			"col":0,
			"text":"请假类型"
		},{
			"type":"radio",
			"row":1,
			"col":1,
			"name":"type",
			"items":"倒休,事假,病假,婚假",
			"required":true,
			"readOnly":false
		},{
			"type":"label",
			"row":2,
			"col":0,
			"text":"开始时间"
		},{
			"type":"datepicker",
			"row":2,
			"col":1,
			"name":"startDate",
			"required":true,
			"readOnly":false
		},{
			"type":"select",
			"row":2,
			"col":2,
			"name":"startDateTime",
			"items":"上午,下午",
			"required":true,
			"readOnly":false
		},{
			"type":"label",
			"row":3,
			"col":0,
			"text":"结束时间"
		},{
			"type":"datepicker",
			"row":3,
			"col":1,
			"name":"endDate",
			"required":true,
			"readOnly":false
		},{
			"type":"select",
			"row":3,
			"col":2,
			"name":"endDateTime",
			"items":"下午,上午",
			"required":true,
			"readOnly":false
		},{
			"type":"label",
			"row":4,
			"col":0,
			"text":"部门经理意见"
		},{
			"type":"radio",
			"row":4,
			"col":1,
			"name":"leaderComment",
			"items":"同意,不同意",
			"required":false,
			"readOnly":true
		},{
			"type":"label",
			"row":5,
			"col":0,
			"text":"人事意见"
		},{
			"type":"radio",
			"row":5,
			"col":1,
			"name":"hrComment",
			"items":"同意,不同意",
			"required":false,
			"readOnly":true
		},{
			"type":"label",
			"row":6,
			"col":0,
			"text":"调整申请"
		},{
			"type":"radio",
			"row":6,
			"col":1,
			"name":"initiatorComment",
			"items":"重新申请,撤销申请",
			"required":true,
			"readOnly":false
		}]
	}]
}
